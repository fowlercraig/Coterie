/* Reset */

.accordion,
.accordion ul,
.accordion li,
.accordion a,
.accordion span {
  margin: 0;
  padding: 0;
  border: none;
  outline: none;
  margin-bottom: 0;
}

.accordion { margin: 20px 0 0! important; }

.accordion li {
  list-style: none;
  border-top: 1px solid {{ settings.dotted_color }};
  margin-bottom: 0 !important;
}

/* Layout & Style */

.accordion li > a {
  display: block;
  position: relative;
  min-width: 110px;
  padding: 10px 0;
  height: 40px;
  text-decoration: none;
  background: url(plus.png) no-repeat;
  background-position: center right;
}

.accordion > li:target > a,
.accordion > li > a.active {
  background: url(minus.png) no-repeat;
  background-position: right;
}

/* Sub Menu */

.sub-menu ul {
  border: 0;
}

.sub-menu li {
  border: 0;
}

.sub-menu em {
  position: absolute;
  top: 0;
  left: 0;
  margin-left: 14px;
  color: #a6a6a6;
  font: normal 10px/32px Arial, sans-serif;
}

/* Functionality */

.accordion li > .sub-menu {
  display: none;
}

.accordion li:target > .sub-menu {
  display: block;
}




